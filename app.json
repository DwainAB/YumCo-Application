{
  "expo": {
    "name": "YumCo",
    "slug": "eatopia",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "yumco",
    "userInterfaceStyle": "automatic",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eatopia.app",
      "deploymentTarget": "15.1",
      "buildNumber": "2",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Cette application nécessite l'accès à votre photothèque pour vous permettre de sélectionner et partager vos photos de plats et expériences culinaires"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo-adaptative.png",
        "backgroundColor": "#fff"
      },
      "package": "com.eatopia.app",
      "permissions": [
        "WAKE_LOCK",
        "ACCESS_NETWORK_STATE",
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED",
        "FOREGROUND_SERVICE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "yumco"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "extra": {
      "eas": {
        "projectId": "c3231076-739a-4599-a861-9539f5cdabc6"
      }
    },
    "owner": "dwain"
  }
}
